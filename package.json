{
  "name": "parcel-ts-boilerplate",
  "version": "1.0.0",
  "scripts": {
    "dev": "parcel ./public/index.html",
    "build": "rimraf -rf dist && parcel build ./public/index.html --experimental-scope-hoisting",
    "test": "jest",
    "plugin": "cd ./parcel-plugin-demo && npm link && cd ../ && sudo yarn add file:./parcel-plugin-demo/"
  },
  "staticFiles": {
    "staticPath": [
      {
        "staticPath": "static",
        "staticOutDir": "static"
      }
    ],
    "watcherGlob": "**"
  },
  "alias": {
    "@/pages": "./src/pages",
    "@/components": "./src/components"
  },
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "babel-jest": "^26.6.0",
    "babel-plugin-import": "^1.13.1",
    "less": "^3.12.2",
    "parcel-bundler": "^1.12.4",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-router-dom": "^5.2.0",
    "typescript": "^4.0.3"
  },
  "devDependencies": {
    "@ice/spec": "^1.0.1",
    "@jest/core": "^26.6.0",
    "@testing-library/react-hooks": "^3.4.2",
    "@types/enzyme": "^3.10.7",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.10",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.6",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "eslint": "^7.11.0",
    "jest": "^26.6.0",
    "jest-enzyme": "^7.1.2",
    "parcel-plugin-bundle-visualiser": "^1.2.0",
    "parcel-plugin-demo": "file:./parcel-plugin-demo/",
    "react-test-renderer": "^16.14.0",
    "rimraf": "^3.0.2",
    "stylelint": "^13.7.2",
    "ts-jest": "^26.4.1"
  }
}
