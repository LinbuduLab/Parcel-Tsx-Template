{
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false
      }
    ],
    ["@babel/preset-react"],
    ["@babel/preset-typescript"]
  ],
  "plugins": [
    [
      "import",
      {
        "libraryName": "antd",
        "libraryDirectory": "es",
        "style": "css"
      }
    ],
    [
      "babel-plugin-styled-components",
      {
        "minify": false,
        "transpileTemplateLiterals": false,
        "pure": true,
        "displayName": false
      }
    ]
  ]
}
